    <attr name="colorWifiConnected" format="reference|color" />
    <attr name="colorPreferenceOption" format="reference|color" />
    <attr name="colorDialogBackgroundNormal" format="reference|color" />
    <attr name="colorDialogDivider" format="reference|color" />
    <attr name="wifiSwitch" format="reference" />
    <attr name="colorDivider" format="reference|color" />
    <attr name="imageview_color" format="color|reference" />

    <attr name="colorListItemBackgroundStroke" format="color|reference" />
    <attr name="colorListItemBackgroundNormal" format="color|reference" />
    <attr name="colorListItemBackgroundPressed" format="color|reference" />
    <attr name="colorListItemBackgroundActivated" format="color|reference" />

    <attr name="colorListItemRecordBackgroundStroke" format="color|reference" />
