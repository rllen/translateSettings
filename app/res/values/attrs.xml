    <attr name="colorDividerBetweenHeaderAndContent" format="color|reference" />
    <attr name="colorLanguageTitle" format="color|reference" />
    <attr name="colorLanguageDesc" format="color|reference" />

    <attr name="layout_marginLeft" format="dimension|reference" />
    <attr name="layout_marginRight" format="dimension|reference" />
